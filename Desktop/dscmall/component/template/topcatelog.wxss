.topcatelog{
  box-sizing: border-box;
  width: calc(100% - 40rpx);
  margin: 20rpx;
  padding: 20rpx;
  background-color: #ffffff;
  display: flex;
  justify-content: start;
  flex-wrap: wrap;
  align-items: center;
  border-radius: 20rpx;
}
.topcatelog_item{
  width: 20%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  text-align: center;
  margin-bottom: 20rpx;
}
.img{
  width: 100%;
  height: 150rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
}
.img image{
  width: 80%;
  height: 80%;
}
.name{
  font-size: 24rpx;
  text-align: center;
  line-height: 24rpx;
}