.skill{
  margin: 20rpx;
  width: calc(100% - 40rpx);
  background-color: #fff;
  border-radius: 20rpx;
}
.items{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: start;
  justify-items: center;
}
.skill .box{
  width: 20%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.skill .img{
  width: 100%;
  height: 100%;
}
.name{
  width: 100%;
  margin-top: 4rpx;
  font-size: 26rpx;
  height: 32rpx;
  text-align: center;
  font-size: 26rpx;
  line-height: 32rpx;
}